<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Braze.InAppMessage.Full" parent="@style/Braze.InAppMessage">
        <item name="android:background">@color/com_braze_inappmessage_background_light</item>
        <item name="android:padding">0.0dip</item>
        <item name="android:layout_width">450.0dip</item>
        <item name="android:layout_height">720.0dip</item>
        <item name="android:layout_centerInParent">true</item>
    </style>
    <style name="Widget.Design.TabLayout" parent="@style/Base.Widget.Design.TabLayout">
        <item name="tabGravity">center</item>
        <item name="tabMode">fixed</item>
    </style>
    <style name="ia_expand_collapse_button_style" parent="@style/ia_top_right_overlay">
        <item name="android:background">@drawable/ia_round_overlay_bg</item>
        <item name="android:padding">@dimen/ia_image_control_padding</item>
        <item name="android:layout_width">@dimen/ia_image_control_size</item>
        <item name="android:layout_height">@dimen/ia_image_control_size</item>
        <item name="android:src">@drawable/ia_sel_expand_collapse</item>
        <item name="android:scaleType">fitCenter</item>
    </style>
    <style name="ia_tv_skip_style" parent="@style/ia_top_right_overlay">
        <item name="android:textAppearance">@style/ia_video_overlay_text_view</item>
        <item name="android:ellipsize">end</item>
        <item name="android:gravity">center</item>
        <item name="android:background">@drawable/bg_text_overlay</item>
        <item name="android:padding">@dimen/ia_video_text_padding</item>
        <item name="android:layout_width">@dimen/ia_video_overlay_button_width</item>
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_marginRight">0.0dip</item>
        <item name="android:maxLines">1</item>
    </style>
</resources>
